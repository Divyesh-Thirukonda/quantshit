# Environment Variables for Vercel + Supabase Deployment

# Development environment
ENVIRONMENT=development

# Supabase Configuration (set these in Vercel dashboard)
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=INFO

# Health Monitoring
HEALTH_CHECK_INTERVAL=30
METRICS_RETENTION_HOURS=24